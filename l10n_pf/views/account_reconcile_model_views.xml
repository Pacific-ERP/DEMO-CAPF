<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="view_account_reconcile_model_search_inherited" model="ir.ui.view">
            <field name="name">account.reconcile.model.search</field>
            <field name="model">account.reconcile.model</field>
            <field name="inherit_id" ref="account.view_account_reconcile_model_search" />
            <field name="arch" type="xml">
                <filter name="withtax" position="before">
                    <field name="match_journal_ids" string="Journaux" filter_domain="" />
                </filter>
            </field>
        </record>
        <record id="view_account_reconcile_model_tree_inherited" model="ir.ui.view">
            <field name="name">account.reconcile.model.tree</field>
            <field name="model">account.reconcile.model</field>
            <field name="inherit_id" ref="account.view_account_reconcile_model_tree" />
            <field name="arch" type="xml">
                <field name="rule_type" position="before">
                    <field name="auto_reconcile" />
                    <field name="match_nature" />
                    <field name="match_label" />
                    <field name="match_label_param" />
                </field>
            </field>
        </record>
    </data>
</odoo>